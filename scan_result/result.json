{
	"options": {
		"fastlane": {
			"title": "Fatsfile's path",
			"value_map": {
				"fastlane/Fastfile": {
					"title": "fastlane lane",
					"env_key": "FASTLANE_LANE",
					"value_map": {
						"beta": {
							"config": "fastlane-config"
						},
						"deploy": {
							"config": "fastlane-config"
						},
						"test": {
							"config": "fastlane-config"
						},
						"test_and_snapshot": {
							"config": "fastlane-config"
						}
					}
				}
			}
		},
		"ios": {
			"title": "Project (or Workspace) path",
			"env_key": "BITRISE_PROJECT_PATH",
			"value_map": {
				"fastlane.xcodeproj": {
					"title": "Scheme name",
					"env_key": "BITRISE_SCHEME",
					"value_map": {
						"Example": {
							"config": "ios-test-config"
						}
					}
				}
			}
		}
	},
	"configs": {
		"fastlane": {
			"fastlane-config": {
				"format_version": "1.1.0",
				"default_step_lib_source": "https://github.com/bitrise-io/bitrise-steplib.git",
				"app": {},
				"workflows": {
					"primary": {
						"steps": [
							{
								"git-clone@3.1.1": {
									"source": {},
									"deps": {}
								}
							},
							{
								"certificate-and-profile-installer@1.4.0": {
									"source": {},
									"deps": {}
								}
							},
							{
								"fastlane@2.1.3": {
									"source": {},
									"deps": {},
									"inputs": [
										{
											"lane": "$FASTLANE_LANE"
										}
									]
								}
							}
						]
					}
				}
			}
		},
		"ios": {
			"ios-config": {
				"format_version": "1.1.0",
				"default_step_lib_source": "https://github.com/bitrise-io/bitrise-steplib.git",
				"app": {},
