					"primary": {
						"steps": [
							{
								"git-clone@3.1.1": {
									"source": {},
									"deps": {}
								}
							},
							{
								"certificate-and-profile-installer@1.4.0": {
									"source": {},
									"deps": {}
								}
							},
							{
								"xcode-archive@1.7.0": {
									"source": {},
									"deps": {},
									"inputs": [
										{
											"project_path": "$BITRISE_PROJECT_PATH"
										},
										{
											"scheme": "$BITRISE_SCHEME"
										}
									]
								}
							},
							{
								"deploy-to-bitrise-io@1.2.2": {
									"source": {},
									"deps": {}
								}
							}
						]
					}
				}
			},
